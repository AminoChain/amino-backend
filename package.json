    "start": "tsc && node build/amino-backend/src/app.js",
    "dev": "nodemon src/app.ts",
    "test": "nyc ./node_modules/.bin/_mocha 'test/**/*.ts'"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.3.0",
    "@types/express": "^4.17.14",

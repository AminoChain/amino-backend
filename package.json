{
  "name": "amino-backend",
  "dependencies": {
    "@types/aes-js": "^3.1.1",
    "@types/cors": "^2.8.12",
    "aes-js": "^3.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "keccak256": "^1.0.6",
    "web3.storage": "^4.4.0"
  },
  "scripts": {
    "update-artifacts": "rm -r src/artifacts; mkdir src/artifacts && cp -r ../amino-contracts/artifacts/contracts/* src/artifacts && git commit src/artifacts/**/*.json -m artifacts",
